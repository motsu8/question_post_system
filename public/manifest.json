{
  "manifest_version": 3,
  "name": "question_post_to_discord",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs",
    "nativeMessaging"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://recursionist.io/dashboard/*/*"],
      "run_at": "document_idle",
      "js": ["content.js"]
    }
  ]
}